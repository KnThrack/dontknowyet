{
	"plugins": ["plugins/markdown"],
	"recurseDepth": 10,
	"opts": {
		"recurse": true,
		"destination": "./docs/",
		"template": "node_modules/docdash",
		"verbose": true
	},
	"source": {
		"include": ["src", "src/views"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(node_modules/|docs)"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},

	"docdash": {
		"static": [true],
		"sort": [true],
		"sectionOrder": ["Classes", "Modules", "Externals", "Events", "Namespaces", "Mixins", "Tutorials", "Interfaces"],
		"disqus": "",
		"openGraph": {
			"title": "Not sure yet application",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
		"search": [true],
		"collapse": [true],
		"wrap": [true],
		"typedefs": [true],
		"navLevel": [4],
		"private": [true],
		"removeQuotes": ["all"],
		"scripts": [],
		"menu": {
			"Project Website": {
				"href": "https://myproject.com",
				"target": "_blank",
				"class": "menu-item",
				"id": "website_link"
			},
			"Forum": {
				"href": "https://myproject.com.forum",
				"target": "_blank",
				"class": "menu-item",
				"id": "forum_link"
			}
		}
	}
}
